{
  "name": "your-project",
  "packageManager": "npm@11.4.2",
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:nest\"",
    "dev:next": "cd apps/next-app/front-end && npm run dev",
    "dev:nest": "cd apps/nest-app/back-end && npm run dev",
    "build": "turbo run build",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "npm-run-all": "^4.1.5",
    "turbo": "^2.5.4"
  }
}
